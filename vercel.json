{
  "version": 2,
  "builds": [
    {
      "src": "api/bot.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/scheduled_task.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/bot",
      "dest": "api/bot.py"
    },
    {
      "src": "/scheduled-task",
      "dest": "api/scheduled_task.py"
    }
  ]
}